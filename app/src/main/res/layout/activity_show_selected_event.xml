<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.nightout.ofek.nightout.ShowSelectedEvent">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/visitHTextView"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/addressTextView"
        android:layout_marginBottom="17dp"
        android:layout_marginEnd="303dp"
        android:layout_marginRight="303dp"
        android:layout_marginTop="8dp"
        android:text="Address:"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        android:id="@+id/DontTouch3" />

    <TextView
        android:id="@+id/DontTouch4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/nameTextView"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/nameTextView"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="30dp"
        android:layout_marginTop="79dp"
        android:text="Name:"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintRight_toLeftOf="@+id/nameTextVE"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.946" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/ageTextV"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/capTextV"
        android:text="Capacity:"
        android:textSize="20sp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:id="@+id/textView4"
        android:layout_marginBottom="19dp"
        app:layout_constraintBottom_toTopOf="@+id/DontTouch2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/phoneNumTextView"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/ageTextV"
        android:text="Age:"
        android:textSize="20sp"
        android:id="@+id/DontTouch2"
        android:layout_marginRight="343dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintVertical_bias="0.362"
        android:layout_marginEnd="343dp" />

    <TextView
        android:layout_width="69dp"
        android:layout_height="27dp"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/phoneNumTextView"
        android:layout_toStartOf="@+id/phoneNumTextView"
        android:text="Price"
        android:textSize="20sp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:id="@+id/DontTouch"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="230dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/DontTouch2"
        app:layout_constraintVertical_bias="0.126" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/addressTextView"
        android:layout_alignTop="@+id/ageTextV"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="241dp"
        android:layout_marginTop="8dp"
        android:layout_toEndOf="@+id/ageTextV"
        android:text="+"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.362"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="241dp"
        android:id="@+id/textView5" />

    <TextView
        android:id="@+id/DontTouch6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        android:text="publicity"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/DontTouch"
        app:layout_constraintVertical_bias="0.038" />


    <TextView
        android:id="@+id/nameTextVE"
        android:layout_width="115dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="79dp"
        android:ems="10"
        android:hint="@string/NAME"
        android:inputType="textNoSuggestions"
        android:text="Name"
        android:textSize="18sp"
        app:layout_constraintHorizontal_bias="0.32"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/priceTextVE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:hint="@string/price"
        android:inputType="number"
        android:text="Price"
        android:textSize="18sp"
        android:layout_marginTop="12dp"
        app:layout_constraintTop_toBottomOf="@+id/ageTextVE"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/DontTouch"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="0.178" />

    <TextView
        android:id="@+id/addressTextVE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:inputType="textPostalAddress"
        android:text="address"
        android:textSize="18sp"
        android:layout_marginTop="14dp"
        app:layout_constraintTop_toBottomOf="@+id/nameTextVE"
        android:layout_marginRight="80dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintLeft_toRightOf="@+id/DontTouch3"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="0.0" />

    <TextView
        android:id="@+id/capTextVE"
        android:layout_width="113dp"
        android:layout_height="21dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:hint="@string/capacity"
        android:inputType="number"
        android:text="Capacity"
        android:textSize="18sp"
        android:layout_marginTop="21dp"
        app:layout_constraintTop_toBottomOf="@+id/addressTextVE"
        app:layout_constraintLeft_toRightOf="@+id/textView4"
        android:layout_marginLeft="8dp" />

    <TextView
        android:id="@+id/descriptionTextVE"
        android:layout_width="368dp"
        android:layout_height="183dp"
        android:layout_marginTop="17dp"
        android:autoLink="web|email|phone"
        android:ems="10"
        android:hint="@string/write_somthing_to_describe_your_club"
        android:inputType="textLongMessage"
        android:lines="5"
        android:overScrollMode="ifContentScrolls"
        android:text="Description"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:textSize="16sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/publicityTextVE"
        android:layout_marginLeft="-14dp" />

    <TextView
        android:id="@+id/ageTextVE"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:ems="10"
        android:hint="@string/min_age"
        android:text="Age"
        android:textSize="18sp"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/DontTouch2"
        android:layout_marginLeft="47dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.14"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/capTextVE"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.037" />

    <TextView
        android:id="@+id/typeTextVE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="-7dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="67dp"
        android:ems="10"
        android:hint="@string/line"
        android:inputType="textPersonName"
        android:text="Type"
        android:textSize="18sp"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toRightOf="@+id/textView9"
        app:layout_constraintRight_toRightOf="@+id/priceTextVE"
        app:layout_constraintTop_toBottomOf="@+id/priceTextVE" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="23dp"
        android:ems="10"
        android:hint="@string/line"
        android:inputType="textPersonName"
        android:text="publicity"
        android:textSize="18sp"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toRightOf="@+id/textView9"
        app:layout_constraintRight_toRightOf="@+id/priceTextVE"
        app:layout_constraintTop_toBottomOf="@+id/priceTextVE" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Type"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="330dp" />

</android.support.constraint.ConstraintLayout>
